{
    "description": "convolutional spelling error detection model that distinguishes between real words and non-words generated by applying edits learned from real spelling errors",
    "train_path": "data/train.h5",
    "validation_path": "data/validation.h5",
    "mode": "transient",
    "n_classes": 2,
    "class_weight_exponent": 1,
    "n_epochs": 100,
    "save_best_only": true,
    "confusion_matrix": true,
    "classification_report": true,
    "checkpoint_name": "model.h5",
    "patience": 20,
    "seed": 17,
    "batch_size": 128,
    "target_name": "binary_target",
    "data_name": ["marked_chars"],

    "n_embed_dims": 10,
    "n_filters": 2000,
    "filter_width": 6,
    "fully_connected": [1000,1000],
    "n_residual_blocks": 0,
    "n_highway": 0,
    "n_embeddings": 255,

    "train_embeddings": true,
    "embedding_init": "uniform",
    "batch_normalization": false,

    "optimizer": "Adamax",
    "loss": "categorical_crossentropy",
    "l2_penalty": 0.0,

    "dropout_embedding_p": 0.0,
    "dropout_conv_p": 0.0,
    "dropout_fc_p": 0.0,

    "embedding_max_norm": 1000,
    "filter_max_norm": 1000,
    "dense_max_norm": 1000,
    "clipnorm": 0,
    "border_mode": "valid"
}
